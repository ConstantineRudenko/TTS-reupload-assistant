{
    "configurations": [
        {
            "name": "Launch Program",
            "outFiles": ["${workspaceFolder}/build/**/*.js"],
            "preLaunchTask": "npm: build" /* built-in */,
            "program": "${workspaceFolder}/src/main.ts",
            "request": "launch",
            "args": [
                "%USERPROFILE%/Documents/My Games/Tabletop Simulator/Saves/TS_Save_105.json",
                "%USERPROFILE%/Documents/My Games/Tabletop Simulator/Mods/",
                "%USERPROFILE%/Documents/My Games/Tabletop Simulator/Backup_tmp",
                "--timeout=500",
                "--no-links",
                "--simultanenous=1"
            ],
            "runtimeExecutable": "node",
            "skipFiles": ["<node_internals>/**"],
            "smartStep": true,
            "sourceMaps": true,
            "type": "node"
        }
    ]
}

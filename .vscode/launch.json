{
    "configurations": [
        {
            "name": "Launch Program",
            "outFiles": ["${workspaceFolder}/build/**/*.js"],
            "preLaunchTask": "npm: build" /* built-in */,
            "program": "${workspaceFolder}/src/main.ts",
            "request": "launch",
            "args": [
                "%USERPROFILE%/Documents/My Games/Tabletop Simulator/Saves/TS_Save_106.json",
                "%USERPROFILE%/Documents/My Games/Tabletop Simulator/Mods/",
                "%USERPROFILE%/Documents/My Games/Tabletop Simulator/ModBackup",
                "--timeout=500",
                "--simultaneous=5",
                "--no-links"
            ],
            "runtimeExecutable": "node",
            //"skipFiles": ["<node_internals>/**"],
            "skipFiles": [],
            "smartStep": true,
            "sourceMaps": true,
            "type": "node"
        }
    ]
}
